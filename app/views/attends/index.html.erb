<h1>Listing attends</h1>

<table>
  <tr>
    <th>Player id</th>
    <th>Trainingevent id</th>
    <th>Attendance</th>
    <th>Excuse</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @attends.each do |attend| %>
  <tr>
    <td><%= attend.player_ID %></td>
    <td><%= attend.trainingEvent_ID %></td>
    <td><%= attend.attendance %></td>
    <td><%= attend.excuse %></td>
    <td><%= link_to 'Show', attend %></td>
    <td><%= link_to 'Edit', edit_attend_path(attend) %></td>
    <td><%= link_to 'Destroy', attend, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Attend', new_attend_path %>
